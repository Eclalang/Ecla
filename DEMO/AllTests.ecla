# test for i
# ----------------------------------------------------------------------------------------------------------------

console.println("for 1");
for (var i int = 0, i < 10, i++) {
    console.println(i);
}

console.println("for 2");
for (var i int = 0, i > 10, i--) {
    console.println(i);
}

console.println("for 3");
for (var i int = 0, i <= 10, i++) {
    console.println(i);
}

console.println("for 4");
for (var i int = 0, i >= 10, i--) {
    console.println(i);
}

