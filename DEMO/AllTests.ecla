# test while
# ----------------------------------------------------------------------------------------------------------------

var aa int;
var bb int = 5;
var cc int;
var dd int = 5;


console.println("while 1");
while(aa < 5) {
    console.println(aa);
    aa++;
}

console.println("while 2");
while(bb > 0) {
    console.println(bb);
    bb--;
}

console.println("while 3");
while(cc <= 5) {
    console.println(cc);
    cc++;
}

console.println("while 4");
while(dd >= 0) {
    console.println(dd);
    dd--;
}


